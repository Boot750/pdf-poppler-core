{
  "name": "pdf-poppler",
  "version": "0.2.1",
  "description": "Convert PDF files into images using Poppler with promises. It achieves 10x faster performance compared to other PDF converters. \nPoppler library attached inside statically, so it has not require installation of poppler. Supports Windows, macOS, and Linux including AWS Lambda.",
  "main": "index.js",
  "scripts": {
    "setup:test": "bash scripts/setup-test.sh",
    "build:core": "cd packages/pdf-poppler-core && npm install && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "npm run setup:test && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boot750/pdf-poppler-core.git"
  },
  "homepage": "https://github.com/Boot750/pdf-poppler-core#readme",
  "bugs": {
    "url": "https://github.com/Boot750/pdf-poppler-core/issues"
  },
  "keywords": [
    "pdf",
    "poppler",
    "jpeg",
    "jpg",
    "png",
    "tiff",
    "tif",
    "parse",
    "info",
    "image",
    "pdf2image",
    "pdf2img",
    "electron",
    "lambda",
    "aws",
    "linux",
    "serverless"
  ],
